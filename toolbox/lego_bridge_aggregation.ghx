    if aggregation_id is None:
        aggregation_id = 'myAggregation'
    
    if reset is None:
        reset = False
    
    if check_data:
        ## create aggregation in sticky dict
        if aggregation is None or aggregation == -1 or reset:
            
            ## copy parts to avoid editing the original parts
            parts_copy = []
            for part in parts:
